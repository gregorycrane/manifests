      "value": {
        "en": [
          "-420"
        ]
      }
    },
    {
      "label": {
        "en": [
          "decoration_description"
        ]
      },
      "value": {
        "en": [
          "<P>Narrow band of upright palmettes, enclosed by tendrils; it is\n      part of the upper frame of the missing figure panel. Part of one lateral frame a thin reserved\n      line is preserved at left. Inscription in added red letters at upper\n      left.</P>"
        ]
      }
    },
